<!DOCTYPE HTML PUBLIC "-//W3C//DTD HTML 4.01//EN" "http://www.w3.org/TR/html4/strict.dtd">
<%@ var body: String %>
<%@ var title: String = "** missing title **" %>
<% escapeMarkup = false %>

<html dir="ltr">

    <head>
        <title>ESB Audit</title>

        <link rel="stylesheet" type="text/css" href="${uri("/theme/main/master.css")}"/>

        <link type="text/css" rel="stylesheet" href="${uri("/theme/SyntaxHighlighter.css")}"></link>
        <script language="javascript" src="${uri("/js/shCore.js")}"></script>
        <script language="javascript" src="${uri("/js/shBrushXml.js")}"></script>
        <script language="javascript">
           dp.SyntaxHighlighter.ClipboardSwf="${uri("/flash/clipboard.swf")}";
           dp.SyntaxHighlighter.HighlightAll('code', 'showColumns');
        </script>

        <link rel="stylesheet" href="${uri("/theme/custom/jquery-ui-1.8.11.custom.css")}">
        <script src="${uri("/js/jquery-1.4.4.js")}"></script>
        <script src="${uri("/js/jquery.ui.core.js")}"></script>
        <script src="${uri("/js/jquery.ui.widget.js")}"></script>
        <script src="${uri("/js/jquery.ui.datepicker.js")}"></script>
        <script src="${uri("/js/jquery.ui.tabs.js")}"></script>
        <script src="${uri("/js/jquery.ui.accordion.js")}"></script>
	<script>
	$(function() {
		$( "#datepicker" ).datepicker();
        $( "#accordion" ).accordion({active: 0 });
        $( "#tabs" ).tabs();
        $( "button, input:submit, a", ".tag-button" ).button();
		$( "a", ".tag-button" ).click(function() { return false; });
	});
	</script>

     </head>

    <body>
    <header>
      <div class="title">
        Apache Servicemix ESB Audit
      </div>
      <div class="search">
        <h2>Search</h2>
      </div>
      <div class="subtitle"></div>
    </header>
    <%= body %>
    <footer>
      <div class="title">Â© 2004-2010 The Apache Software Foundation</div>

    </footer>
    </body>

</html>